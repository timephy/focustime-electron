{
  "name": "focustime",
  "version": "1.0.0",
  "description": "An electron app to set a focus timer.",
  "main": "main.js",
  "build": {
    "appId": "com.timguggenmos.focustime",
    "productName": "Focustime",
    "icon": "images/icon.png",
    "mac": {
      "category": "public.app-category.productivity",
      "target": "dmg"
    }
  },
  "scripts": {
    "start": "electron .",
    "build:mac": "electron-builder -m",
    "build:win": "electron-builder -w",
    "build:linux": "electron-builder -l",
    "build": "electron-builder -mwl",
    "generate:images": "./bin/generate-images.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timephy/focustime.git"
  },
  "author": "Tim Guggenmos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/timephy/focustime/issues"
  },
  "homepage": "https://github.com/timephy/focustime#readme",
  "devDependencies": {
    "electron": "^8.0.0",
    "electron-builder": "^22.3.2",
    "eslint": "^6.8.0",
    "svgexport": "^0.4.0"
  },
  "dependencies": {
    "electron-store": "^5.1.0"
  }
}
