{
  "name": "focustime",
  "version": "1.0.0",
  "description": "An electron app to set a focus timer.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack:osx": "electron-packager . Focustime --out=dist --platform=darwin --arch=x64 --icon=icons/mac/icon.icns --overwrite",
    "pack:win32": "electron-packager . Focustime --out=dist --platform=win32 --arch=ia32 --icon=icons/win/icon.ico --overwrite",
    "pack:win64": "electron-packager . Focustime --out=dist --platform=win32 --arch=x64 --icon=icons/win/icon.ico --overwrite",
    "generate:images": "./bin/generate-images.sh",
    "generate:icons": "./bin/generate-icons.sh",
    "build": "npm run generate:images && npm run generate:icons && npm run pack:osx && npm run pack:win32 && npm run pack:win64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timephy/focustime.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/timephy/focustime/issues"
  },
  "homepage": "https://github.com/timephy/focustime#readme",
  "devDependencies": {
    "electron": "^8.0.0",
    "eslint": "^6.8.0",
    "svgexport": "^0.4.0"
  },
  "dependencies": {
    "electron-store": "^5.1.0"
  }
}
